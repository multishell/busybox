#!/bin/sh
# Make busybox links list file.

DF="busybox.def.h"
MF="busybox.c"

LIST="$(sed -n '/^#define/{s/^#define //p;}' $DF)"

for def in ${LIST}; do
	i=`sed -n 's/^#ifdef \<'$def'\>.*\/\/\(.*$\)/\/\1\//gp' $MF`
	j=`sed -n '/^#ifdef \<'$def'\>.*/,/^#endif/{ s/.*\"\(.*\)\".*/\1/gp; }' $MF`
	for k in $j; do
	    echo $i$k
	done
done
