# Depends on FEATURE_GREP_EGREP_ALIAS
_BB_CONFIG_DEP=FEATURE_GREP_EGREP_ALIAS

testing "grep is also egrep" "foo" "foo\n" "" "foo\nbar\n"
testing "egrep is not case insensitive" "foo ; [ \$? -ne 0 ] && echo yes" \
	"yes\n" "" "FOO\n"


exit $FAILCOUNT
